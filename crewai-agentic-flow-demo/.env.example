# Environment variables for CrewAI Agentic Flow Demo

# OpenAI API Key (primary LLM provider for CrewAI)
OPENAI_API_KEY=your_openai_api_key_here

# Firecrawl API Key (for real web scraping - optional, demo uses mock by default)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Alternative LLM providers (optional)
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GOOGLE_API_KEY=your_google_key_here
# GROQ_API_KEY=your_groq_key_here

# CrewAI Configuration (optional)
# CREWAI_TELEMETRY_ENABLED=false